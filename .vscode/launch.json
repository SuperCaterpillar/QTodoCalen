{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "QML Debug",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/QTodoCalen.exe", // 可执行文件名
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "args": [
        "-qmljsdebugger=host:localhost,port:12150,block,services:DebugMessages,QmlDebugger,V8Debugger,QmlInspector"
      ], 

      "windows": {
        "environment": [
          {
            "name": "PATH",
            "value": "${env:PATH};D:\\dev-env\\Qt\\6.8.3\\msvc2022_64\\bin"
          },
          {
            "name": "QT_QPA_PLATFORM_PLUGIN_PATH",
            "value": "D:\\dev-env\\Qt\\6.8.3\\msvc2022_64\\plugins"
          },
          {
            "name": "QML_IMPORT_PATH",
            "value": "D:\\dev-env\\Qt\\6.8.3\\msvc2022_64\\qml"
          }
        ]
      }
    }
  ],
  "compounds": []
}